# Example: Grading students based on their scores
def grade_students(score):
    if score >= 90:
        grade = 'A'
    elif score >= 80:
        grade = 'B'
    elif score >= 70:
        grade = 'C'
    elif score >= 60:
        grade = 'D'
    else:
        grade = 'F'
    return grade

# Example usage:
score = int(input("Enter the student's score: "))
student_grade = grade_students(score)
print("Student's grade:", student_grade)
